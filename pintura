}
function setup() {

createCanvas(600, 600);

}

function draw() {

background("white");

stroke("blue")

fill("red")

rect(100, 250, 100, 150)

}
function setup() {

createCanvas(600, 600);

background("white");

}

function draw() {

stroke("blue");

fill("red");

if(mouseIsPressed){

rect(mouseX, mouseY, 20, 35);

}

}
